.settingslandscape#settingslandscape
   .close
      img#settingslandscapeclose(src="images/ui/desktop/CH_desktop_close.png")

   div.header
      span Settings

   div.header2

      span Bet
      br
      img.leftarrow#betminus(src="images/minus.png")
      input#betslider(type="range",min=0.01,max=1,step=0.01)
      img.rightarrow#betplus(src="images/plus.png")
      br

      span#betamount.normal
         | 0.00
      br
      br
      span Music Volume
      br
      img.leftarrow#musicminus(src="images/minus.png")
      input#musicslider(type="range",min=0,max=1,step=0.01)
      img.rightarrow#musicplus(src="images/plus.png")
      br

      span#musicvolume.normal
         | 0
      br
      br
      span Sound Effects Volume
      br
      img.leftarrow#fxminus(src="images/minus.png")
      input#fxslider(type="range",min=0,max=1,step=0.01)
      img.rightarrow#fxplus(src="images/plus.png")
      br

      span#fxvolume.normal
         | 0

